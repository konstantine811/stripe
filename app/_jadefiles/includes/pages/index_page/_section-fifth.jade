                        .buttons
                            button.bt.purple Explore Solution
                            button.bt.grey Request a demo
                    .tab__image
                        .image-middle
                        .image-txt
                            p.image-txt__inner Img 5
            .tab-pane.fade#clinical-research(role="tabpanel" aria-labelledby="clinical-research-tab")
                .tab__inner
                    .tab-txt
                        .tab-txt__title
                            h3.not-indent Clinical Research
                        .tab-txt__about
