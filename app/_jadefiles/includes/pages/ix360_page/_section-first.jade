                    .head-content-boxes__inner
                        .title-wrap-back.title-wrap-back--blue-light
                            .title-wrap-back__inner
                                h4.text-uppercase.text-bold Interface layer
                        ul.boxes-items
                            each val in boxs_items.interface
                                li.boxes-items__inner
                                    .boxes-items__txt
                                        p.txt-main-title.color-blue-dark #{val}
            .col-lg-3.col-md-6.col-sm-6.col-xs-12
                .head-content-boxes.head-content-boxes--right
                    .head-content-boxes__inner
                        .title-wrap-back.title-wrap-back--blue-purple
                            .title-wrap-back__inner
                                h4.text-uppercase.text-bold Backend layer
                        ul.boxes-items
                            each val in boxs_items.backend
                                li.boxes-items__inner
                                    .boxes-items__txt
                                        p.txt-main-title.color-blue-dark #{val}
            .col-lg-3.col-md-6.col-sm-6.col-xs-12
                .head-content-boxes.head-content-boxes--left
                    .head-content-boxes__inner
                        .title-wrap-back.title-wrap-back--purple
                            .title-wrap-back__inner
                                h4.text-uppercase.text-bold Science & Regulatory
                        ul.boxes-items
                            each val in boxs_items.science
                                li.boxes-items__inner
                                    .boxes-items__txt
                                        p.txt-main-title.color-blue-dark #{val}
            .col-lg-3.col-md-6.col-sm-6.col-xs-12
                .head-content-boxes.head-content-boxes--right
                    .head-content-boxes__inner
                        .title-wrap-back.title-wrap-back--pink
                            .title-wrap-back__inner
                                h4.text-uppercase.text-bold Operation layer
                        ul.boxes-items
                            each val in boxs_items.operation
                                li.boxes-items__inner
                                    .boxes-items__txt
                                        p.txt-main-title.color-blue-dark #{val}
                                    
                               



                
                        
                        
                        
                                
                     


            
